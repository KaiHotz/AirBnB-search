{
  "name": "react-redux-airbnb-search",
  "version": "1.0.0",
  "description": "React Redux AirBnB Search",
  "main": "index.js",
  "repository": "https://github.com/KaiHotz/AirBnB-search",
  "homepage": "https://KaiHotz.github.io/AirBnB-search",
  "author": "Kai Hotz",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --mode development",
    "watch": "npm run start -- --watch",
    "lint": "esw --ext .jsx --ext .js --color",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage --colors",
    "build": "npm run clean && webpack --config ./config/webpack.config.prod.js --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "eslint-watch": "^3.1.5",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-expect": "^3.8.3",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-jsonp": "^1.1.3",
    "formik": "^0.11.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.4.0",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "yup": "^0.25.1"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}

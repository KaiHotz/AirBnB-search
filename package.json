{
  "name": "react-redux-airbnb-search",
  "version": "1.0.0",
  "description": "React Redux AirBnB Search",
  "main": "index.js",
  "repository": "https://github.com/KaiHotz/AirBnB-search",
  "homepage": "https://KaiHotz.github.io/AirBnB-search",
  "author": "Kai Hotz",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --mode development",
    "lint": "esw --ext .jsx --ext .js --color",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage --colors",
    "build": "npm run clean && webpack --config ./config/webpack.config.prod.js --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-expect": "^3.8.4",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "fetch-jsonp": "^1.1.3",
    "formik": "^1.0.0-alpha.6",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.5.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "yup": "^0.26.6"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
